{
  "name": "@streamdal/snitch-protos",
  "version": "0.0.1",
  "description": "Snitch Protobufs",
  "main": "protos/external_api_client.js",
  "scripts": {
    "build:node": "npm run clean && npm run type:dts && npm run build:compile",
    "build:compile": "cd ./protos/node && swc ./ -d ./node/protos",
    "clean": "rimraf build coverage nyc_output",
    "build:deno": "node esm_module_extensions.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type:dts": "tsc --emitDeclarationOnly --project tsconfig.build.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streamdal/snitch-protos.git"
  },
  "keywords": [
    "Snitch",
    "Protobuf",
    "Streamdal"
  ],
  "author": "Streamdal",
  "license": "MIT",
  "type": "module",
  "bugs": {
    "url": "https://github.com/streamdal/snitch-protos/issues"
  },
  "homepage": "https://github.com/streamdal/stitch-protos/build/ts/node",
  "dependencies": {
    "@grpc/grpc-js":"^1.8.18",
    "@protobuf-ts/plugin": "^2.9.0"
  },
  "devDependencies": {
    "glob": "^10.3.3",
    "rimraf": "^5.0.1",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.70",
    "typescript": "^5.1.6"
  }
}
